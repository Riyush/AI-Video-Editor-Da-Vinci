{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "core:default"
    },
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://*"
        }
      ]
    },
    "dialog:default",
    "fs:default"
  ]
}